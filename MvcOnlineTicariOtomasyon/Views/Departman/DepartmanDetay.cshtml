@model List<Personel>

@{
    ViewBag.Title = "DepartmanDetay";
}
<br />
<h2>Departman Detay Sayfası</h2>
<br />

<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active">Departman Adı: @ViewBag.v1</li>

</ol>
<table class="table table-hover">

    <tr>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Satışlar</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.PersonelAd</td>
            <td>@item.personelSoyad</td>
            <td><a href="/Departman/DepartmanSatıslar/@item.Personelİd" class="btn btn-default" style="background-color:brown; color:white">Satışlar</a></td>
        </tr>
    }
</table>